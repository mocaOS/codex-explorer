  "scripts": {
    "build": "nuxt build",
    "dev": "npm run fix:unenv && nuxt dev --port 3001",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare && npm run fix:unenv",
    "fix:unenv": "node -e \"const fs=require('fs');const path=require('path');const src='node_modules/unenv/dist/runtime/mock';const dest='node_modules/unenv/dist/runtime/runtime';if(fs.existsSync(src)&&!fs.existsSync(dest+'/mock')){fs.mkdirSync(dest,{recursive:true});fs.cpSync(src,dest+'/mock',{recursive:true})}\""
  },
